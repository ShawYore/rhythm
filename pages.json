{
	"easycom": {
		"autoscan": true,
		  "custom": {
		    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue", // 匹配components目录内的vue文件
		    "^vue-file-(.*)": "packageName/path/to/vue-file-$1.vue" // 匹配node_modules内的vue文件
		  }
		
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		}, {
			"path": "pages/record/index",
			"style": {
				"navigationBarTitleText": "实盘记录"
			}
		}, {
			"path": "pages/mine/index",
			"style": {
				"navigationBarTitleText": "我的"
			}
		}, {
			"path": "pages/rank/index",
			"style": {
				"navigationBarTitleText": "排行榜"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F5F5F5"
	},
	"uniIdRouter": {},
	"tabBar": {
		"color": "#707070",
		"selectedColor": "#FFB900",
		"borderStyle": "white",
		"backgroundColor": "#F5F5F5",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "/static/image/tabbar/home.png",
			"selectedIconPath": "/static/image/tabbar/home-s.png",
			"text": "首页"
		}, {
			"pagePath": "pages/rank/index",
			"iconPath": "/static/image/tabbar/rank.png",
			"selectedIconPath": "/static/image/tabbar/rank-s.png",
			"text": "排行榜"
		}, {
			"pagePath": "pages/mine/index",
			"iconPath": "/static/image/tabbar/mine.png",
			"selectedIconPath": "/static/image/tabbar/mine-s.png",
			"text": "我的"
		}]
	}
}